@using Website.Helpers
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <title>@ViewBag.Title</title>
    <link rel="icon" type="image/png" href="@Url.Content("~/assets/images/favico.png")">
    <link href='https://fonts.googleapis.com/css?family=Raleway:300' rel='stylesheet' type='text/css'>
    @Styles.Render("~/Content/css")
    @RenderSection("Header", false)

    <script>
        (function () {
            var _fbq = window._fbq || (window._fbq = []);
            if (!_fbq.loaded) {
                var fbds = document.createElement('script');
                fbds.async = true;
                fbds.src = '//connect.facebook.net/en_US/fbds.js';
                var s = document.getElementsByTagName('script')[0];
                s.parentNode.insertBefore(fbds, s);
                _fbq.loaded = true;
            }
            _fbq.push(['addPixelId', '883873758344579']);
        })();
            window._fbq = window._fbq || [];
            window._fbq.push(['track', 'PixelInitialized', {}]);
    </script>
    <noscript><img height="1" width="1" alt="" style="display:none" src="https://www.facebook.com/tr?id=883873758344579&amp;ev=PixelInitialized" /></noscript>
</head>
<body>

    <div class="row above-header-CTA hidden-md hidden-lg">
        <div class="hidden-md col-sm-12 col-xs-12">
            <a href="@Url.Action("DiscussProject", "Lead")"><button class="CTA-button-full-head-small-devices">DISCUSS A PROJECT</button></a>
        </div>
    </div>

    <nav class="navbar navbar-default" id="NavigationSection">
        <div class="header-container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="@Url.Action("Index", "Home")"><img src="@Url.Content("~/assets/images/logo.png")" alt="" style="width: 100%;"></a>
            </div>
            <li class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                <ul class="nav navbar-nav navbar-right">
                    <li class="@Html.IsActive("Index", "Home")">@Html.ActionLink("Home", "Index", "Home")</li>
                    <li class="@Html.IsActive("About", "Home")">@Html.ActionLink("Solving development", "About", "Home")</li>
                    <li class="@Html.IsActive("References", "Home")">@Html.ActionLink("References", "References", "Home")</li>
                    <li class="dropdown">
                        <a class="@Html.IsActive("Index", "Page") btn btn-primary dropdown-toggle button-dropdown" href="@Url.Action("Index", new {controller = "Page", slug = "resources"})">
                            Resources
                            <span class="caret"></span>
                        </a>
                        <ul class="dropdown-menu">
                            <li class="@Html.IsActive("Index", "Page")">@Html.ActionLink("Services", "Index", "Page", new { slug = "resources" }, new { })</li>
                            <li class="divider"></li>
                            <li><a href="#">Blog</a></li>
                        </ul>
                    </li>
                    <li class="@Html.IsActive("Contact", "Home")">@Html.ActionLink("Contact", "Contact", "Home", null, new { @class = "header-li-margin-element" })</li>
                    <li>
                        <a href="@Url.Action("DiscussProject", "Lead")" style="padding: 0;"><button class="CTA-button-full-head">DISCUSS A PROJECT</button></a>
                    </li>
                </ul>
            </div>
        </div>
    </nav>




    @RenderBody()

