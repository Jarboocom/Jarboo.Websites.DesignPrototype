                <div class="col-md-2 hidden-sm hidden-xs">
                    <img src="@Url.Content("~/assets/images/logo.png")" alt=" ">
                </div>
            </div>
            <div class="col-md-12 col-sm-12 col-xs-12 section1-text-footer-social">
                <div class="social-icons">
                    <div class="social-icons-facebook">
                        <a href="https://www.facebook.com/jarboocom"><img src="@Url.Content("~/assets/images/facebook.png")" alt="facebook"></a>
                    </div>
