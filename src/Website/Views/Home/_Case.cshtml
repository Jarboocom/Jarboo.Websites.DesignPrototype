@model Services.Domain.Case.Case

<div class="col-md-6 col-sm-12 col-xs-12">
    <div class="case-study-shell">
        <div class="col-md-3 col-sm-3 col-xs-12 case-study-img">
            <div class="case-study-holder"></div>
        </div>
        <div class="col-md-9 col-sm-8 col-xs-12 case-study-text">
            <h3 class="case-study-head">@Model.Title</h3>
            <p class="case-study-p">@Model.Description</p>
            <a href="@Url.Action("Index", new {controller = "Case", slug = Model.Slug})"><button class="case-study-full-button">READ MORE</button></a>
        </div>
    </div>
</div>